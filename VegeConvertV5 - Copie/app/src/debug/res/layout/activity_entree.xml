<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".entree">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/backslow" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="240dp"
        android:layout_height="57dp"
        android:fontFamily="@font/nerko_one"
        android:gravity="center"
        android:text="ENTREE"
        android:textColor="#F57E0F"
        android:textSize="52dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <EditText
        android:id="@+id/MainNameText"
        android:layout_width="368dp"
        android:layout_height="54dp"
        android:fontFamily="@font/montserrat_alternates"
        android:hint="Research"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.134" />

    <ImageView
        android:id="@+id/arrow_right"
        android:layout_width="66dp"
        android:layout_height="54dp"
        android:background="@drawable/arrow_right"
        app:iconGravity="textEnd"
        app:iconTint="#A91D58"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.954"
        app:rippleColor="#03A9F4" />

    <ImageView
        android:id="@+id/arrow_left"
        android:layout_width="66dp"
        android:layout_height="54dp"
        android:background="@drawable/arrow_left"
        app:iconGravity="textEnd"
        app:iconTint="#A91D58"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.954"
        app:rippleColor="#03A9F4" />

    <ImageView
        android:id="@+id/house"
        android:layout_width="66dp"
        android:layout_height="54dp"
        android:background="@drawable/house"
        app:iconGravity="textEnd"
        app:iconTint="#A91D58"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:rippleColor="#03A9F4" />

    <ListView
        android:id="@+id/ListViewEntree"
        android:layout_width="389dp"
        android:layout_height="453dp"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="159dp"
        tools:ignore="MissingConstraints">

    </ListView>


</androidx.constraintlayout.widget.ConstraintLayout>